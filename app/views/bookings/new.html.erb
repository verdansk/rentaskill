<div class="container">
  <div class="row">
    <div class="col-12">
      <%= simple_form_for([@skill,@booking]) do |f| %>
      <%= f.input :date_start, as: :date %>
      <%= f.input :date_end, as: :date %>
      <%= f.input :price %>
      <%= f.submit "New booking", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
